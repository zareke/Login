@{
    ViewData["Title"] = "Index";
}
<h1>@ViewBag.Error</h1>
<div id="dataUsuarios"></div>
<script src="@Url.Content("https://localhost:7254/js/site.js?v=HiQ1At_J5cUTV1hndDMwCbtlbM1gy7Kn23IXKiONu1k")"></script>

<h1>a!</h1>

<form method="post" id="login" name="login" action='@Url.Action("Check","Account")' onsubmit="return validarFormulario()">
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Usuario</label>
    <input name="Username" type="text" class="form-control" id="user" aria-describedby="emailHelp">
  </div>
  
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">Password</label>
    <input name="Contraseña" type="password" class="form-control" id="exampleInputPassword1" onkeyup="ValidacionContraseña(Contraseña, ochoContainer, especialContainer, mayuscMinuscContainer)">
    <p>La contraseña debe:</p>
          <ul class="list-group">
            <li class="list-group-item">
              <input class="form-check-input me-1" name="ochoContainer" type="checkbox" value="" id="firstCheckboxStretched" onclick="return false;">
              <label class="form-check-label stretched-link" for="firstCheckboxStretched">Mas de 8 caracteres</label>
            </li>
            <li class="list-group-item">
              <input class="form-check-input me-1" name="especialContainer" type="checkbox" value="" id="secondCheckboxStretched" onclick="return false;">
              <label class="form-check-label stretched-link" for="secondCheckboxStretched">Al menos un caracter especial</label>
            </li>
            <li class="list-group-item">
              <input class="form-check-input me-1" name="mayuscMinuscContainer" type="checkbox" value="" id="thirdCheckboxStretched" onclick="return false;">
              <label class="form-check-label stretched-link" for="thirdCheckboxStretched">Al menos 1 mayuscula y 1 minuscula</label>
            </li>
          </ul>
  </div>
  
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Email</label>
    <input name="Email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
  </div>

  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Fecha de nacimiento</label>
    <input  onchange="ValidarNacimiento(FechaNacimiento, textito)" name="fechaNacimiento" type="date" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <p id="textito" name="FechaNacimiento" hidden="true">El usuario debe tener mas de 5 años y menos de 110</p>
  </div>
  
  <div class="mb-3">
    <!--PONER EL SELECT AL LADO DEL INPUT TEL-->
    <label for="exampleInputEmail1" class="form-label">Telefono</label><br/>
    <select>
      <option value="+54">+54</option>
      <option value="+1">+1</option>
      <option value="+23">+23</option>
      <option value="+43">+43</option>
    </select>

    <input onkeyup="ValidarTelefono(NumeroTelefono,textitoTel)" name="NumeroTelefono" type="number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <p id="textitoTel" name="textitoTel" hidden="true">El numero de telefono no es valido</p>

  </div><!-- https://es.sprint.com/en/support/solutions/international/international-mobile-dial-codes.html -->
  
  <div class="mb-3 form-check">
    <input type="checkbox" class="form-check-input" id="exampleCheck1">
    <label class="form-check-label" for="exampleCheck1">Acepto los <a href="https://www.argentina.gob.ar/terminos-y-condiciones" target="_blank">terminos y condiciones</a></label>
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>